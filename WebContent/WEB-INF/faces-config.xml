<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<managed-bean>
		<managed-bean-name>calculadoraBean</managed-bean-name>
		<managed-bean-class>CalculadoraBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule> 
    <from-view-id>/CalculadoraView.xhtml</from-view-id> 
    <navigation-case> 
      <from-outcome>calculadoraBean.somar</from-outcome> 
      <to-view-id>/CalculadoraView.xhtml</to-view-id> 
    </navigation-case> 
  </navigation-rule>  

</faces-config>
